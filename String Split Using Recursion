--Column Name: N
--Table Name: RC

WITH T(START_POS, END_POS, D) AS (

SELECT 1 AS START_POS, INSTR(N,',') AS END_POS,
SUBSTR(N, 1, INSTR(N, ',')-1) AS D FROM RC

UNION ALL

SELECT T.END_POS+1,
INSTR(N, ',', T.END_POS+1),
CASE WHEN INSTR(N, ',', T.END_POS+1) <> 0 
THEN SUBSTR(N, INSTR(N, ',', T.END_POS)+1, INSTR(N, ',', T.END_POS+1)-(T.END_POS+1))
ELSE SUBSTR(N, INSTR(N, ',', -1)+1) END
FROM T, RC
WHERE T.END_POS>0 )

SELECT D FROM T;
